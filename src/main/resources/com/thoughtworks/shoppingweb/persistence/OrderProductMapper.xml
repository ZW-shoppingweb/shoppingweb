<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"

        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.thoughtworks.shoppingweb.persistence.OrderProductMapper">
    <insert id="insertProductToOrder"  parameterType="OrdersProduct">
        INSERT INTO ordersProduct(orderId,productId,productNum)
        VALUES (#{orderId},#{productId},#{productNum})
    </insert>

</mapper>
